<h1>
  <%= @company.name %>
</h1>
<h2>
  <%= @company.ticker_symbol %>
</h2>
<h3>
  <%= @company.risk_factor %>
</h3>

<table>
  <thead>
    <tr>
      <th>
        Date
      </th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    <% @company.stock_prices.each do |stock_price| %>
      <tr>
        <td>
          <%= stock_price.captured_at %>
        </td>
        <td>
          <%= stock_price.price %>
        </td>
      </tr>
      <% end %>
  </tbody>
</table>